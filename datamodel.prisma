type User {
  id: ID! @id
  username: String! @unique
  password: String!
  accs: [Acc] @relation(name: "Acc" ,onDelete: CASCADE )
  taxAccs: [TaxAcc] @relation(name: "TaxAcc" ,onDelete: CASCADE )
  taxLaw1s: [TaxLaw1] @relation(name: "TaxLaw1" ,onDelete: CASCADE )
  taxLaw2s: [TaxLaw2] @relation(name: "TaxLaw2" ,onDelete: CASCADE )
  totalAccs: [TotalAcc] @relation(name: "TotalAcc" ,onDelete: CASCADE) 
  createdAt: DateTime @createdAt
  updatedAt: DateTime @updatedAt
}

type Acc {
  id: ID! @id
  user: User! @relation(name: "Acc")
  score: Float!
  round: Int!
  episode: Int!
  academy: String!
  rank: Int
  createdAt: DateTime @createdAt
  updatedAt: DateTime @updatedAt
}

type TaxAcc {
  id: ID! @id
  user: User! @relation(name: "TaxAcc")
  score: Float!
  round: Int!
  episode: Int!
  academy: String!
  rank: Int
  createdAt: DateTime @createdAt
  updatedAt: DateTime @updatedAt
}

type TaxLaw1 {
  id: ID! @id
  user: User! @relation(name: "TaxLaw1")
  score: Float!
  round: Int!
  episode: Int!
  academy: String!
  rank: Int
